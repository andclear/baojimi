# Supabase 数据库配置（必填）
SUPABASE_URL="your_supabase_project_url"
SUPABASE_ANON_KEY="your_supabase_anon_key"

# 管理后台密码（必填，建议设置复杂一点）
PASSWORD="your_super_secure_password"

# 默认访问密钥（可选，但建议填写）
DEFAULT_ACCESS_KEY="sk-your-default-access-key"

# 速率限制配置（可选，有默认值）
MAX_REQUESTS_PER_MINUTE=30
MAX_REQUESTS_PER_DAY_PER_IP=2000

# 日志配置（可选）
MAX_LOG_COUNT=300